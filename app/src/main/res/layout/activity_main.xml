<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity"
    android:orientation="vertical">
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity"
    android:orientation="vertical">


    <ImageView
        android:layout_height="170dp"
        android:layout_width="170dp"
        android:background="@drawable/a"
        android:id="@+id/topleft"
        />

    <ImageView
        android:layout_height="90dp"
        android:layout_width="170dp"
        android:background="@drawable/b"
        android:layout_toRightOf="@id/topleft"
        android:id="@+id/topright"
        />

    <ImageView
        android:layout_height="90dp"
        android:layout_width="85dp"
        android:background="@drawable/c"
        android:layout_below="@id/topright"
        android:layout_toRightOf="@id/topleft"
        android:id="@+id/belowtopright"
        />

    <ImageView
        android:layout_height="90dp"
        android:layout_width="85dp"
        android:background="@drawable/d"
        android:layout_below="@id/topright"
        android:layout_toRightOf="@id/belowtopright"
        android:id="@+id/ab"
        />

    <ImageView
        android:layout_height="match_parent"
        android:layout_width="170dp"
        android:background="@drawable/e"
        android:layout_below="@id/ab"
        android:layout_toRightOf="@id/topleft"
        android:id="@+id/belowright"
        />


    <ImageView
        android:layout_height="170dp"
        android:layout_width="85dp"
        android:background="@drawable/f"
        android:layout_below="@id/topleft"
        android:layout_toLeftOf="@id/belowright"
        android:id="@+id/blue"
        />

    <ImageView
        android:layout_height="85dp"
        android:layout_width="85dp"
        android:background="@drawable/g"
        android:layout_below="@id/topleft"
        android:layout_toLeftOf="@id/blue"
        android:id="@+id/bluegreen"
        />

    <ImageView
        android:layout_height="85dp"
        android:layout_width="85dp"
        android:background="@drawable/h"
        android:layout_below="@id/bluegreen"
        android:layout_toLeftOf="@id/blue"
        android:id="@+id/blackgreen"
        />


    <ImageView
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:background="@drawable/f"
        android:layout_below="@id/blackgreen"
        android:layout_toLeftOf="@id/belowright"
        android:id="@+id/hush"
        />

</RelativeLayout>

    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
        android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity"
        android:orientation="vertical">

    <ImageView
        android:layout_height="85dp"
        android:layout_width="85dp"
        android:background="@drawable/j"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_marginRight="5dp"
        android:layout_marginLeft="15dp"
        />

 </RelativeLayout>




</FrameLayout>